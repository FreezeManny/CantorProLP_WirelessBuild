#include <dt-bindings/zmk/matrix_transform.h>
#include <cantor-layouts.dtsi>
&physical_layout0 {
    transform = <&default_transform>;
};

// Keyscan Setup
/ {
    chosen {
        zmk,kscan = &kscan0;
        zmk,physical-layout = &physical_layout0;
    };

    default_transform: keymap_transform_0 {
        compatible = "zmk,matrix-transform";
        columns = <42>;
        rows = <1>;

// | SW00 | SW01 | SW02 | SW03 | SW04 | SW05 |   | SW05  | SW04  | SW03  | SW02  | SW01  | SW00  |
// | SW10 | SW11 | SW12 | SW13 | SW14 | SW15 |   | SW15 | SW14 | SW13 | SW12  | SW11  | SW10  |
// | SW20 | SW21 | SW22 | SW23 | SW24 | SW25 |   | SW25 | SW24 | SW23 | SW22 | SW21 | SW20 |
//                      | SW30 | SW31 | SW32 |   | SW32 | SW31 | SW30 |

        map = <
            RC(0,0)  RC(0,1)  RC(0,2)  RC(0,3)  RC(0,4)  RC(0,5)     RC(0,26) RC(0,25) RC(0,24) RC(0,23) RC(0,22) RC(0,21)
            RC(0,6)  RC(0,7)  RC(0,8)  RC(0,9)  RC(0,10) RC(0,11)    RC(0,32) RC(0,31) RC(0,30) RC(0,29) RC(0,28) RC(0,27)
            RC(0,12) RC(0,13) RC(0,14) RC(0,15) RC(0,16) RC(0,17)    RC(0,38) RC(0,37) RC(0,36) RC(0,35) RC(0,34) RC(0,33)
                                       RC(0,18) RC(0,19) RC(0,20)    RC(0,41) RC(0,40) RC(0,39)
        >;
    };

    kscan0: kscan {
        compatible = "zmk,kscan-gpio-direct";
        label = "KSCAN";
        wakeup-source;
        input-gpios =
            <&gpio0 29 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW00
            <&gpio0 20 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW01
            <&gpio0 31 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW02
            <&gpio0 17 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW03
            <&gpio0 8  (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW04
            <&gpio0 6  (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW05

            <&gpio0 22 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW10
            <&gpio0 2  (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW11
            <&gpio0 24 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW12
            <&gpio1 15 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW13
            <&gpio1 0  (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW14
            <&gpio1 13 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW15

            <&gpio1 6  (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW20
            <&gpio0 9  (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW21
            <&gpio0 10 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW22
            <&gpio1 4  (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW23
            <&gpio1 11 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW24
            <&gpio0 11 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW25

            <&gpio1 7  (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW30
            <&gpio1 2  (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>, //SW31
            <&gpio1 1  (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>; //SW32
    };

};
// Keyscan Setup
/ {
    chosen {
        zmk,kscan = &default_kscan;
        zmk,physical-layout = &default_transform;
    };

    default_transform: keymap_transform_0 {
        compatible = "zmk,matrix-transform";
        columns = <42>;
        rows = <1>;
// | SW00 | SW01 | SW02 | SW03 | SW04 | SW05 |   | SW05  | SW04  | SW03  | SW02  | SW01  | SW00  |
// | SW10 | SW11 | SW12 | SW13 | SW14 | SW15 |   | SW15 | SW14 | SW13 | SW12  | SW11  | SW10  |
// | SW20 | SW21 | SW22 | SW23 | SW24 | SW25 |   | SW25 | SW24 | SW23 | SW22 | SW21 | SW20 |
//                      | SW30 | SW31 | SW32 |   | SW32 | SW31 | SW30 |
        map = <
RC(0,0) RC(0,1) RC(0,2) RC(0,3) RC(0,4) RC(0,5)  RC(0,6) RC(0,7) RC(0,8) RC(0,9) RC(0,10) RC(0,11)
RC(0,0) RC(0,1) RC(0,2) RC(0,3) RC(0,4) RC(0,5)  RC(0,6) RC(0,7) RC(0,8) RC(0,9) RC(0,10) RC(0,11)
RC(0,0) RC(0,1) RC(0,2) RC(0,3) RC(0,4) RC(0,5)  RC(0,6) RC(0,7) RC(0,8) RC(0,9) RC(0,10) RC(0,11)
                        RC(0,3) RC(0,4) RC(0,5)  RC(0,6) RC(0,7) RC(0,8)
        >;
    };


};
